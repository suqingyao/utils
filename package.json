{
  "name": "@suqingyao/core",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "personal monorepo",
  "author": "suqingyao",
  "license": "MIT",
  "keywords": [
    "monorepo",
    "utils",
    "typescript",
    "tools"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "changeset": "changeset && changeset version",
    "release": "pnpm run build && changeset version && changeset publish"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "turbo": "^1.10.15",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.14.0"
}